<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
	 <class name="com.Shanklish.Controller.BookmarkedJob" table="BookmarkedJobs">  
		 <id name="id" column="id" type="int"> <generator class="increment"></generator></id>
		 	
		  <property name = "uID" column="uID" type="int"></property>
		 <property name="url" column="savedJob" type="string"></property> 
		 <property name="JobTitle" column="JobTitle" type="string"></property> 
	 </class>      
 </hibernate-mapping>  